{
  "manifest_version": 3,
  "name": "Customer Information Extractor",
  "version": "1.0",
  "description": "Extracts customer information from web pages",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
